\begin_layout Subsection
\noindent
English definition of neighbourhood rule
\end_layout

\begin_layout Standard
\noindent
Swap the crucibles for any two pots which have different crucibles
\end_layout

\begin_layout Subsection
\noindent
Formal neighbour definition
\end_layout

\begin_layout Standard
\noindent
\begin_inset Formula $y(x,i,j)=(x_{1},x_{2},...,x_{i-1},x_{j},x_{i+1},...x_{j-1},x_{i},x_{j+1},...x_{n})$
\end_inset


\end_layout

\begin_layout Subsection
\noindent
Formal neighbourhood definition
\end_layout

\begin_layout Standard
\noindent
\begin_inset Formula $N(x)=\{y(x,i,j),\,i=1,...,n,\,j=i+1,...,n;\,(i,j)<>(1,n)$
\end_inset


\end_layout

